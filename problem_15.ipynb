    "import math\n",
    "\n",
    "def Solve15(width, length):\n",
    "    # binomial coeffecient to find the unique paths\n",
    "    res = math.factorial(width + length) / (math.factorial(width) * math.factorial(((width + length)) - width))\n",
    "    return int(res)\n",
    "\n",
    "print(Solve15(20,20))"
   ]
  }
