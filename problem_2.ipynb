    "def Solve2():\n",
    "\n",
    "    fib_nums = [1,2]\n",
    "\n",
    "    while fib_nums[-1] < 4000000:\n",
    "        fib_nums.append(fib_nums[-2]+fib_nums[-1])\n",
    "\n",
    "    fib_nums = fib_nums[:-1]\n",
    "\n",
    "    res = 0\n",
    "\n",
    "    for num in fib_nums:\n",
    "        if num%2 == 0:\n",
    "            res += num\n",
    "    \n",
    "    return res\n",
    "\n",
    "print(Solve2())"
   ]
  }
