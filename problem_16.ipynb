    "def Solve16(n: int):\n",
    "    digitOfNum = str(2**n)\n",
    "    digitSum = 0\n",
    "    for digit in digitOfNum:\n",
    "        digitSum += int(digit)\n",
    "    \n",
    "    return digitSum\n",
    "\n",
    "print(Solve16(1000))"
   ]
  }
