    "def Solve6(x):\n",
    "    sumSquares = 0\n",
    "    squareSum = 0\n",
    "\n",
    "    for i in range(1, x+1):\n",
    "        sumSquares += i*i\n",
    "        \n",
    "    for i in range(1, x+1):\n",
    "        squareSum += i \n",
    "    return squareSum*squareSum - sumSquares\n",
    "\n",
    "print(Solve6(100))"
   ]
